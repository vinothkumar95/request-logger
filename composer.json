    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vinothkumar\\RequestLogger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "vinothkumar95",
            "email": "cvinoth95@outlook.com"
        }
